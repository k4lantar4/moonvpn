# MoonVPN Environment Variables

# Telegram Settings
BOT_TOKEN=your_bot_token_here
ADMIN_TELEGRAM_ID=your_admin_telegram_id
NOTIFICATIONS_CHANNEL_ID=your_channel_id

# MySQL Database Settings
MYSQL_DATABASE=moonvpn
MYSQL_USER=moonvpn_user
MYSQL_PASSWORD=strong_password_here
MYSQL_ROOT_PASSWORD=stronger_root_password_here
DATABASE_URL=mysql+pymysql://${MYSQL_USER}:${MYSQL_PASSWORD}@db:3306/${MYSQL_DATABASE}

# Redis Settings
REDIS_URL=redis://redis:6379/0

# General Settings
DEFAULT_LANGUAGE=fa
TIMEZONE=Asia/Tehran

# Execution Environment (development or production)
ENV=development 